@model List<ProjectNghiPhep.Models.NHANVIEN>
           <div class="container">
               <table class="table table-bordered">
                   <thead>
                       <tr>
                           <th>Mã nhân viên</th>
                           <th>Họ & tên</th>
                           <th>Giới tính</th>
                           <th>Ngày sinh</th>
                           <th>Địa chỉ</th>
                           <th>Số điện thoại</th>
                           <th>Email</th>
                           <th>Chức vụ</th>
                           <th>User name</th>
                           <th>Action</th>
                       </tr>
                   </thead>
                   <tbody>
                       @for (int i = 0; i < Model.Count(); i++)
                       {
                           <tr>
                               <td>@Model[i].MaNV</td>
                               <td>@Model[i].TenNV</td>
                               <td>@Model[i].GioiTinh</td>
                               <td>@Model[i].NgaySinh</td>
                               <td>@Model[i].DiaChi</td>
                               <td>@Model[i].SDT</td>
                               <td>@Model[i].Email</td>
                               <td>@Model[i].chucvu</td>
                               <td>@Model[i].user_name</td>

                               <td>
                                   <a href="@Url.Action("Edit", new { id = Model[i].MaNV })"><i class="glyphicon glyphicon-edit"></i></a>
                                   <a href="@Url.Action("Delete", new { id = Model[i].MaNV })"><i class="glyphicon glyphicon-trash"></i></a>
                               </td>
                            </tr>
                       }
                   </tbody>
               </table>
           </div>

